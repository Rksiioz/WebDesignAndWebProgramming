pipeline {
    agent any

    stages {
        stage('Clone Repository') {
            steps {
                git 'https://github.com/Rksiioz/WebDesignAndWebProgramming.git'
            }
        }

      stage('SonarQube Analysis') {
    steps {
        script {
            def scannerHome = tool 'SonarQube Scanner'
            withSonarQubeEnv('AGENT Sonar') {
                sh """
                ${scannerHome}/bin/sonar-scanner \
                  -Dsonar.projectKey=lab11 \
                  -Dsonar.projectName=lab11 \
                  -Dsonar.projectVersion=1.0.0 \
                  -Dsonar.sources=. \
                  -Dsonar.language=js \
                  -Dsonar.sourceEncoding=UTF-8
                """
            }
        }
    }
}


        stage('Done') {
            steps {
                echo 'HTML/CSS проект успішно завантажено та проаналізовано!'
            }
        }
    }
}
